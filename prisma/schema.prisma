generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Author {
  id      String @id @default(uuid())
  name    String @unique
  country String
  Books   Book []
}

model Book  {
  id        String @id @default(uuid())
  title     String @unique
  rating    Int
  price     Int
  authorId  String
  author    Author  @relation(fields: [authorId], references: [id], onDelete: Cascade)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}